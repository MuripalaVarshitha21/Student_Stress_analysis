{
  "datasets": [
    {
      "name": "825acabf",
      "displayName": "counters",
      "queryLines": [
        "SELECT\r\n",
        "  COUNT(*) AS total_students,\r\n",
        "  ROUND(AVG(stress_level),2) AS avg_stress_level,\r\n",
        "  ROUND(MAX(risk_score),2) AS max_risk_score\r\n",
        "FROM student_stress_clean;"
      ],
      "catalog": "workspace",
      "schema": "students_stress"
    },
    {
      "name": "24aa2d57",
      "displayName": "q4_extracurricular_vs_stress",
      "queryLines": [
        "SELECT\r\n",
        "  extracurricular_freq,\r\n",
        "  COUNT(*) AS num_students,\r\n",
        "  ROUND(AVG(stress_level), 2) AS avg_stress_level\r\n",
        "FROM student_stress_clean\r\n",
        "GROUP BY extracurricular_freq\r\n",
        "ORDER BY extracurricular_freq;\r\n"
      ],
      "catalog": "workspace",
      "schema": "students_stress"
    },
    {
      "name": "c0bc3899",
      "displayName": "q5_risk_score_analysis",
      "queryLines": [
        "SELECT\r\n",
        "  stress_level,\r\n",
        "  ROUND(AVG(wellbeing_score), 2) AS avg_wellbeing_score,\r\n",
        "  ROUND(AVG(academic_stress_index), 2) AS avg_academic_stress_index,\r\n",
        "  ROUND(AVG(risk_score), 2) AS avg_risk_score\r\n",
        "FROM student_stress_clean\r\n",
        "GROUP BY stress_level\r\n",
        "ORDER BY stress_level;\r\n"
      ],
      "catalog": "workspace",
      "schema": "students_stress"
    },
    {
      "name": "296e2ec0",
      "displayName": "funnel chart",
      "queryLines": [
        "SELECT\r\n",
        "  'Total Students' AS stage, COUNT(*) AS total\r\n",
        "FROM student_stress_clean\r\n",
        "UNION ALL\r\n",
        "SELECT 'High Study Load', COUNT(*)\r\n",
        "FROM student_stress_clean\r\n",
        "WHERE study_load >= 4\r\n",
        "UNION ALL\r\n",
        "SELECT 'Poor Sleep', COUNT(*)\r\n",
        "FROM student_stress_clean\r\n",
        "WHERE sleep_quality <= 2\r\n",
        "UNION ALL\r\n",
        "SELECT 'High Stress', COUNT(*)\r\n",
        "FROM student_stress_clean\r\n",
        "WHERE stress_level >= 4;\r\n"
      ],
      "catalog": "workspace",
      "schema": "students_stress"
    },
    {
      "name": "e764dda6",
      "displayName": "filters",
      "queryLines": [
        "SELECT\r\n",
        "  DISTINCT stress_level,\r\n",
        "  sleep_quality,\r\n",
        "  study_load,\r\n",
        "  extracurricular_freq\r\n",
        "FROM\r\n",
        "  student_stress_clean;\r\n"
      ],
      "catalog": "workspace",
      "schema": "students_stress"
    }
  ],
